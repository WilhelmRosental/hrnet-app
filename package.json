    "@vitejs/plugin-react": "^4.7.0",
    "vite": "^7.0.6",
    "@types/node": "^24.1.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "eslint": "^9.32.0",
    "typescript": "^5.8.3",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3"
